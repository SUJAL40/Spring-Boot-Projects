<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Define all beans -->
    <bean id="disel" class="in.sujal.spring.beans.DiselEngin"></bean>
	<bean id="petrol" class="in.sujal.spring.beans.PetrolEngin"></bean>
	<bean id="car" class="in.sujal.spring.beans.Car" scope="prototype">
	<property name="eng" ref="petrol"></property>
	</bean>


</beans>
